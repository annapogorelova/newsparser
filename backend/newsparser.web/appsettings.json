{
  "AppName": "newsparser",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Security": {
    "AccessTokenLifetimeMinutes": 720,
    "RefreshTokenLifetimeDays": 30
  },
  "AppConfig": {
    "Cache": {
      "DefaultCacheDuration": 5
    }
  }
}